{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Automate Browser",
  "description": "Save your devtoos automations to save time.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage", "declarativeContent", "activeTab", "scripting", "tabs"
    ],
    "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}